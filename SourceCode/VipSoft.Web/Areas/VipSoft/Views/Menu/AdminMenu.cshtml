@model List<VipSoft.Membership.Core.Entity.Menu>
<div id="sidebar">
    <ul>
        @foreach (var menu in Model.FindAll(p => p.ParentId == 201))
        { 
            <li class="submenu">
                <a href="#"><i class="fa fa-th-list"></i><span>@menu.Name</span> <i class="arrow fa fa-chevron-right"></i></a>
                <ul>
                    @foreach (var child in Model.FindAll(p => p.ParentId == menu.ID))
                    {
                        <li><a href="@child.Url" class="ajax-link">@child.Name</a></li> 
                    }
                </ul>
            </li>
        }
