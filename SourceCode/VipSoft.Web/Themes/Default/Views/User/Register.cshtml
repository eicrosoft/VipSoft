@{
    ViewBag.Title = "注册是一种态度";
    Layout = "../Shared/_HomeLayout.cshtml";
}

@section Style {
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Themes/Default/JS/VipSoft.CMS.Feedback.js")" type="text/javascript"></script> 
}



<div class="widget-content nopadding">
    <p>@ViewBag.title</p>
                    <form id="frmUser" method="post" class="form-horizontal"> 
                        <div class="form-group">
                            <label class="control-label"><label for="UserName">用户名</label> </label>
                            <div class="controls">
                                <input id="UserName" name="UserName" type="text" value="">
                            </div>
                        </div> 
                        <div class="form-group">
                            <label class="control-label"><label for="Password">密码</label></label>
                            <div class="controls">
                                <input id="Password" name="Password" type="password" value="">
                            </div>
                        </div>  
                        <div class="form-group">
                            <label class="control-label"><label for="Password">密码</label></label>
                            <div class="controls">
                                <input id="Password1" name="Password1" type="password" value="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label"><label for="RealName">真实姓名</label></label>
                            <div class="controls">
                                <input id="RealName" name="RealName" type="text" value="">
                            </div>
                        </div> 
                        <div class="form-group">
                            <label class="control-label"><label for="NickName">昵称</label></label>
                            <div class="controls">
                                <input id="NickName" name="NickName" type="text" value="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label"><label for="CardNo">证件号码</label></label>
                            <div class="controls">
                                <input id="CardNo" name="CardNo" type="text" value="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label"><label for="Province">省</label> </label>
                            <div class="controls">
                                <input id="Province" name="Province" type="text" value="">
                                <label for="City">市</label>
                                <input id="City" name="City" type="text" value="">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label"><label for="Address">地址</label></label>
                            <div class="controls">
                                <input id="Address" name="Address" type="text" value="">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label"><label for="ZipCode">邮编</label> </label>
                            <div class="controls">
                                <input id="ZipCode" name="ZipCode" type="text" value="">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label"><label for="Tel">电话</label> </label>
                            <div class="controls">
                                <input id="Tel" name="Tel" type="text" value="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label"><label for="Mobile">手机</label> </label>
                            <div class="controls">
                                <input id="Mobile" name="Mobile" type="text" value="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label"><label for="Fax">传真</label> </label>
                            <div class="controls">
                                <input id="Fax" name="Fax" type="text" value="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label"><label for="Email">邮箱</label> </label>
                            <div class="controls">
                                <input id="Email" name="Email" type="text" value="">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label"><label for="QQ">QQ</label></label>
                            <div class="controls">
                                <input id="QQ" name="QQ" type="text" value="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label"></label>
                            <div class="controls">
                                <input id="ID" name="ID" type="hidden" value="">
                                <input type="submit" name="btnSubmit" value="提交保存" id="btnSubmit" class="btn btn-primary">
                                &nbsp;<input name="重置" type="reset" class="btn btn-danger" value="重 置">
                            </div>
                        </div>
                    </form>
                </div>